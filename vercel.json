{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["nrt1"],
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_NAME": "ALSOK IoT騒音監視システム",
      "NEXT_PUBLIC_VERSION": "1.0.1"
    }
  }
}